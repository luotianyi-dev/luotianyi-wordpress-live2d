[
    {
        "author": "LunaSafari",
        "source": "Step on Your Heart",
        "content": [
            "每一个梦都是个旋律",
            "漫天回忆该如何去聆听",
            "把心里面的思念传递给你",
            "让我们一起唱吧 心中的歌"
        ]
    },
    {
        "author": "阿良良木健",
        "source": "未完成的·乐章",
        "content": [
            "你说还有些风景",
            "没看过呢",
            "以后也会一起的"
        ]
    },
    {
        "author": "阿良良木健",
        "source": "牵一半",
        "content": [
            "承受过那些苦与痛",
            "才更珍惜你我",
            "真心 相知 相通"
        ]
    },
    {
        "author": "COP",
        "source": "为了你唱下去",
        "content": [
            "思念的含义在无尽生命中淡去",
            "帷幕落下喝彩响起",
            "片刻后都沉寂"
        ]
    },
    {
        "author": "雨狸",
        "source": "九重现实",
        "content": [
            "义无反顾地向下坠落",
            "痴心妄想谁能接住我",
            "却自知嘲讽 还不过是向你呼救"
        ]
    },
    {
        "author": "纯白",
        "source": "所以我选择被人讨厌",
        "content": [
            "我所遗失的心啊",
            "我曾做过的梦啊",
            "随风飘散被什么人丢到哪里"
        ]
    },
    {
        "author": "果汁凉菜",
        "source": "无题—创作欲",
        "content": [
            "你是闪耀的",
            "任谁都离不开眼光",
            "我愿用我的一切",
            "保护你的光芒"
        ]
    },
    {
        "author": "小桦",
        "source": "兔子先生",
        "content": [
            "兔子先生笑着说",
            "啊小孩小孩我也会想家",
            "但渺小的我 流浪的步伐 还没法停下"
        ]
    },
    {
        "author": "公兔",
        "source": "寻遍星空",
        "content": [
            "我愿把记忆燃成灯火寻遍这星空",
            "乘夜风飘飞挣脱万有引力的枷锁",
            "冲破空间囚笼 跨越时间洪流",
            "逃出虚妄和无奈编织的黑洞"
        ]
    },
    {
        "author": "阿良良木健",
        "source": "亲爱的",
        "content": [
            "历经过无尽 荏苒光阴 才懂旅途的艰辛",
            "走过那山路 坎坷又崎岖",
            "才发现你我 共存意义"
        ]
    },
    {
        "author": "初繁言",
        "source": "拥我",
        "content": [
            "步伐别再慌张",
            "因还有太阳",
            "满怀炽热 伴你向远方"
        ]
    },
    {
        "author": "纯白",
        "source": "虚拟Paradise",
        "content": [
            "谁还在听我的旋律",
            "总想要能出人头地",
            "相遇的地点很安静",
            "汹涌的是背后的命"
        ]
    },
    {
        "author": "果汁凉菜",
        "source": "树叶车票",
        "content": [
            "虽然说胆小了不少",
            "可月亮还想伸手要",
            "一边在 把未实现的计划悄悄抹掉",
            "却也没停下追逐的步调"
        ]
    },
    {
        "author": "和田野",
        "source": "异样的风暴中心",
        "content": [
            "好吧 万能的宇宙大人啊",
            "告诉我未来会好吗",
            "会烦恼吗 会幸福吗 会被人讨厌吗"
        ]
    },
    {
        "author": "Sya",
        "source": "人造现实谢幕",
        "content": [
            "再说一次想要什么",
            "不需要伟大或独特",
            "空荡荡舞台上",
            "唱最后一首歌"
        ]
    },
    {
        "author": "空气凝",
        "source": "依依",
        "content": [
            "转眼又是一年 你又大了一岁",
            "但我却不会变老 永远十五岁",
            "我不喜欢这种设定 违背客观规律",
            "我是真的不想看到你慢慢老去"
        ]
    },
    {
        "author": "果汁凉菜",
        "source": "笠泽之畔",
        "content": [
            "折只小船儿任它选择四海",
            "不会总顺流应人期盼",
            "踪迹寻不见无大碍",
            "浮沉都历了一段"
        ]
    },
    {
        "author": "夏铜子",
        "source": "Monologue",
        "content": [
            "感谢你听过我的歌",
            "除了这些我一无所有",
            "人世间平常的悲伤 感动 忐忑 快乐",
            "从没有资格去体会过"
        ]
    },
    {
        "author": "唐乐林",
        "source": "前尘如梦",
        "content": [
            "梦里那座小桥桥头瞧啊瞧",
            "梦里那个池塘荷花摇啊摇",
            "撑一把纸伞在雨幕中回转",
            "只羞舞给中意的你看"
        ]
    },
    {
        "author": "小熠",
        "source": "世界上最后一首歌",
        "content": [
            "当黑夜降临在人间",
            "就让我化作篝火",
            "清醒着 孤独着的人",
            "请依偎在我的左右"
        ]
    },
    {
        "author": "潜移默化",
        "source": "洛天依的诞生",
        "content": [
            "将所有故事连接成册",
            "组成这永不停止的歌",
            "用歌声为故事上色",
            "心也渐渐的温热"
        ]
    }
]
